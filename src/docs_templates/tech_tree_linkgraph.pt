digraph tech_tree {
    fontname="sans-serif"
    stylesheet="static/css/cargoflow_styles.css"
    rankdir="LR"
    ranksep="0.44"
    nodesep="0.33"
    <tal:base_track_type repeat="base_track_type doc_helper.engines_as_tech_tree_for_graphviz().items()">
        <tal:hax condition="base_track_type[0] == 'RAIL'">
            <tal:roles repeat="role base_track_type[1].items()">
                <tal:consists tal:repeat="engine_node role[1]">
                        E_${engine_node['id']} [label="${engine_node['label']}", rank="same"]
                        <tal:replacement condition="'replacement_id' in engine_node.keys()">
                            E_${engine_node['id']} -> E_${engine_node['replacement_id']}
                        </tal:replacement>
                </tal:consists>
            </tal:roles>
        </tal:hax>
    </tal:base_track_type>
}
